  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}\\bin\\www",
      "env": {
        "CANCREATEUSER": "TRUE",
        "CANDELETEENTITIES": "TRUE",
        "NODE_ENV": "development",
      },
      "restart": true,
      "runtimeExecutable": "C:\\Program Files\\nodejs\\node.exe",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}