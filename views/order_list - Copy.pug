extends layout

block content
  h1= title
  
  ul
  each order in order_list
    li 
      a(href=order.url) #{order.Description}  
      | (#{order._id})
      p #{order.Value}
      p #{order.DateOrder_formatted}
      p #{order.ParentTag}
      p #{order.Tags}
      #container.col
      if order.IsJourney
        p IsJourney
      
      

  else
    li There are no orders.